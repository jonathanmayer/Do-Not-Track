{
   "name": "Do Not Track",
   "description": "Adds the Do Not Track header (DNT: 1) to all requests.",
   "icons": {
      "48": "bug_48.png",
      "128": "bug_128.png"
   },
   "version": "0.1.1",
   "manifest_version": 2,
   "background": {
      "page": "background.html"
   },
   "permissions": [
      "webRequest",
      "<all_urls>",
      "webRequestBlocking"
   ],
   "minimum_chrome_version": "17.0"
}
